{
  "name": "sora-devtools",
  "version": "2024.2.3",
  "description": "WebRTC SFU Sora DevTools",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shiguredo/sora-devtools.git"
  },
  "author": "Shiguredo Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://discord.gg/shiguredo"
  },
  "homepage": "https://github.com/shiguredo/sora-devtools#readme",
  "scripts": {
    "next": "next",
    "dev": "next dev -p 3333",
    "build": "next build --no-lint",
    "dist": "pnpm run cleanup && pnpm install && next build --no-lint",
    "fmt": "biome format --write ./src",
    "lint": "biome lint ./src",
    "fix": "biome check --apply ./src",
    "test": "vitest run",
    "check": "tsc --noEmit",
    "e2e-test": "playwright test --project=chromium",
    "cleanup": "git clean -ffdx --exclude .env"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.4.0",
    "@shiguredo/light-adjustment": "2024.1.0",
    "@shiguredo/mp4-media-stream": "2024.2.0",
    "@shiguredo/noise-suppression": "2022.4.2",
    "@shiguredo/virtual-background": "2023.2.0",
    "bootstrap": "5.3.3",
    "query-string": "9.1.1",
    "react": "18.3.1",
    "react-bootstrap": "2.10.6",
    "react-dom": "18.3.1",
    "react-draggable": "4.4.6",
    "react-redux": "9.1.2",
    "redux": "5.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "3.1.0",
    "sora-js-sdk": "2024.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@playwright/test": "1.49.0",
    "@types/node": "22.10.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-redux": "7.1.34",
    "@types/redux-logger": "3.0.13",
    "@vitejs/plugin-react": "4.3.4",
    "dotenv": "16.4.5",
    "jsdom": "25.0.1",
    "typescript": "5.7.2",
    "vite": "6.0.1",
    "vitest": "2.1.6"
  },
  "packageManager": "pnpm@9.14.3",
  "engines": {
    "node": ">=20.18"
  }
}