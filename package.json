  "bugs": {
    "url": "https://discord.gg/shiguredo"
  },
  "homepage": "https://github.com/shiguredo/sora-devtools#readme",
  "scripts": {
    "next": "next",
    "dev": "next dev -p 3333",
    "build": "next build --no-lint",
    "dist": "pnpm run cleanup && pnpm install && next build --no-lint",
    "fmt": "biome format --write ./src",
    "lint": "biome lint ./src",
    "fix": "biome check --apply ./src",
    "test": "vitest run",
    "e2e-test": "playwright test --project=chromium",
    "cleanup": "git clean -ffdx"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.7",
    "@shiguredo/light-adjustment": "2023.2.0",
    "@shiguredo/noise-suppression": "2022.4.2",
    "@shiguredo/virtual-background": "2023.2.0",
    "bootstrap": "5.3.3",
    "query-string": "9.1.0",
    "react": "18.3.1",
    "react-bootstrap": "2.10.4",
    "react-dom": "18.3.1",
    "react-draggable": "4.4.6",
    "react-redux": "9.1.2",
    "redux": "5.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "3.1.0",
    "sora-js-sdk": "2024.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@playwright/test": "1.45.3",
    "@types/node": "22.1.0",
    "@types/react": "18.3.3",
    "@types/react-redux": "7.1.33",
    "@types/redux-logger": "3.0.13",
    "@vitejs/plugin-react": "4.3.1",
    "dotenv": "16.4.5",
    "jsdom": "24.1.1",
    "next": "14.2.5",
    "typescript": "5.5.4",
    "vitest": "2.0.4"
  },
  "packageManager": "pnpm@9.3.0",
  "engines": {
    "node": ">=18"
  }
}