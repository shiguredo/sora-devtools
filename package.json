  "scripts": {
    "next": "next",
    "dev": "next dev -p 3333",
    "build": "next build --no-lint",
    "dist": "pnpm run cleanup && next build --no-lint",
    "fmt": "biome format --write ./src",
    "lint": "biome lint ./src",
    "fix": "biome check --apply ./src",
    "test": "vitest run",
    "cleanup": "rimraf ./.next ./dist/*",
    "knip": "knip",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.1.0",
    "@shiguredo/light-adjustment": "2023.2.0",
    "@shiguredo/noise-suppression": "2022.4.2",
    "@shiguredo/virtual-background": "2023.2.0",
    "bootstrap": "5.3.2",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-bootstrap": "2.10.0",
    "react-dom": "18.2.0",
    "react-draggable": "4.4.6",
    "react-redux": "9.1.0",
    "redux": "5.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "3.1.0",
    "sora-js-sdk": "2023.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "20.11.10",
    "@types/react": "18.2.48",
    "@types/react-redux": "7.1.33",
